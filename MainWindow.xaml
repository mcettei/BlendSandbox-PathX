								</DoubleAnimationUsingKeyFrames>
								<DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="border">
									<EasingDoubleKeyFrame KeyTime="0" Value="1"/>
								</DoubleAnimationUsingKeyFrames>
							</Storyboard>
						</VisualState>
						<VisualState x:Name="Pressed">
							<Storyboard>
								<ThicknessAnimationUsingKeyFrames Storyboard.TargetProperty="(FrameworkElement.Margin)" Storyboard.TargetName="stackPanel">
									<EasingThicknessKeyFrame KeyTime="0" Value="0,1,-1,0"/>
								</ThicknessAnimationUsingKeyFrames>
								<DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="border">
									<EasingDoubleKeyFrame KeyTime="0" Value="1"/>
								</DoubleAnimationUsingKeyFrames>
							</Storyboard>
						</VisualState>
						<VisualState x:Name="Disabled">
							<Storyboard>
								<ThicknessAnimationUsingKeyFrames Storyboard.TargetProperty="(FrameworkElement.Margin)" Storyboard.TargetName="stackPanel">
									<EasingThicknessKeyFrame KeyTime="0" Value="0,1,-1,0"/>
								</ThicknessAnimationUsingKeyFrames>
								<DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="border">
									<EasingDoubleKeyFrame KeyTime="0" Value="1"/>
								</DoubleAnimationUsingKeyFrames>
							</Storyboard>
						</VisualState>
					</VisualStateGroup>
				</VisualStateManager.VisualStateGroups>
				<StackPanel x:Name="stackPanel" Height="Auto" Width="Auto">
					<StackPanel.Effect>
						<DropShadowEffect BlurRadius="3" ShadowDepth="1" Opacity="0.7"/>
					</StackPanel.Effect>
					<Image Height="50" VerticalAlignment="Bottom" HorizontalAlignment="Center" Width="Auto" Source="icon_Patients.png" Stretch="Uniform"/>
					<Border BorderBrush="#FF6D6D6D" BorderThickness="0" CornerRadius="2" Margin="0" Padding="3,1,3,2" Width="82">
						<Border.Background>
							<LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
								<GradientStop Color="#FFF06E23"/>
								<GradientStop Color="{DynamicResource TopBannerGrad}" Offset="1"/>
								<GradientStop Color="#FFFF843A" Offset="0.486"/>
							</LinearGradientBrush>
						</Border.Background>
						<TextBlock TextWrapping="Wrap" Text="PATIENTS" HorizontalAlignment="Center" Foreground="White"/>
					</Border>
				</StackPanel>
			</Border>
		</ControlTemplate>
